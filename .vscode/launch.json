{
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "vitepress: chrome",
        "url": "http://localhost:5173",
        "webRoot": "${workspaceFolder}",
        "pathMapping": {
          "/_karma_webpack_": "${workspaceFolder}"
        },
        "sourceMapPathOverrides": {
          "webpack:/*": "${webRoot}/*",
          "/./*": "${webRoot}/*",
          "/src/*": "${webRoot}/*",
          "/*": "*",
          "/./~/*": "${webRoot}/node_modules/*"
        },
        "preLaunchTask": "vitepress: dev"
      }
    ]
  }